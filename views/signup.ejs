<body id="signupbody">
<div class="container-fluid ">
        
    
     <div class="d-flex justify-content-center">
     <br>
     
        
    </div>
    <br><br>
 <div class="d-flex justify-content-center">
     <br>
    <h4>Register</h4>
 
    </div>
 <div class="d-flex justify-content-center">
  <form class=" col-lg-6 signupform">
      <br>
          <div class="form-group">
        
      <label for="name" class="labelname">Full Name:</label>
         
      <input type="text" class="form-control  mx-auto namebox " id="name" required>
    </div>
         
          
          <div class="form-group">
        
      <label for="contact" class="labelname">Contact:</label>
         
      <input type="text" class="form-control  mx-auto namebox" id="contact" required>
    </div>
           <div class="form-group">
        
      <label for="address" class="labelname">Address:</label>
         
      <input type="text" class="form-control  mx-auto namebox" id="address" required>
    </div>
    <div class="form-group">
      <label for="username" class="labelname">Username:</label>
      <input type="text" class="form-control  mx-auto namebox" id="username" required>
    </div>
                <div class="form-group">
        
      <label for="email" class="labelname">Email:</label>
      <input type="email" class="form-control  mx-auto namebox" id="email" required>
    </div>
    <div class="form-group">
      <label for="pwd" class="labelname">Password:</label>
      <input type="password" class="form-control  mx-auto namebox" id="password" required>
    </div>
       <div class="d-flex justify-content-center">
     <button type="submit" class="btn btn-success btn-lg" id="submit">Submit</button>
    </div>
      <br> 
        <div class="d-flex justify-content-center">
     <button  class="login100-form-btn" ><a href="/">Login Page</a></button>
    </div>
   <br>
  </form>
    </div>
    <br>
</div>
    </body>
